<section id="cooperation" class="light-gray-bg pv-30 clearfix">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <h2 class="text-center">@(partners.GetPropertyValue<string>("title"))</h2>
                <div class="separator"></div>
            </div>
        </div>
        <div class="row">
            <div class="container">
                <div class="clients-container">
                    <div class="clients">
                        @{
                            object obj = partners.GetPropertyValue("partners");
                            List<string>
                                list = obj.ToString().Split(',').ToList();
                            int a = 1;
                            foreach (var item in list.Skip(0).Take(5))
                            {
                                string time = (a * 200).ToString();
                                <div style="width:160px;height:160px" class="client-image object-non-visible" data-animation-effect="fadeIn" data-effect-delay="@time">
                                    <img src="@(Umbraco.Media(int.Parse(item)).Url)" alt="">
                                </div>
                                a++;
                            }
                            <p></p>
                            foreach (var item in list.Skip(5).Take(5))
                            {
                                string time = (a * 200).ToString();
                                <div style="width:160px;height:160px" class="client-image object-non-visible" data-animation-effect="fadeIn" data-effect-delay="@time">
                                    <img src="@(Umbraco.Media(int.Parse(item)).Url)" alt="">
                                </div>
                                a++;
                            }
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
