<section class="pv-30" id="development">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <h2 class="text-center">@(development.GetPropertyValue<string>("title"))</h2>
                <div class="separator"></div>
                <p class="large text-center">@(development.GetPropertyValue<string>("content").ReomveHtmlAttribute())</p>
            </div>
        </div>
        <div class="row">
            @{
                foreach (IPublishedContent content in development.Children)
                {
                    int linkid = content.GetPropertyValue<int>
                        ("urllink");
                    dynamic childrenContent = Umbraco.Content(linkid);
                    if (childrenContent.Id != 0)
                    {
                        <div class="col-md-4 ">
                            <div class="pv-40 ph-40 light-gray-bg boxed shadow object-non-visible" data-animation-effect="fadeInDownSmall" data-effect-delay="150">
                                <div class="icon without-bg"><i class="@(content.GetPropertyValue<string>("style"))"></i></div>
                                <div class="body">
                                    <h4 class="title text-center">@(content.GetPropertyValue<string>("title"))</h4>
                                    <p class="text-center">@(content.GetPropertyValue<string>("content").ReomveHtmlAttribute())</p>
                                    <p class="text-center"><a href="@(childrenContent.Url)">@(content.GetPropertyValue<string>("urltitle"))</a></p>
                                </div>
                            </div>
                        </div>
                    }

                }
            }
        </div>
    </div>
</section>
