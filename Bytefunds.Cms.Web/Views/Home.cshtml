@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "_Layout.cshtml";


    IPublishedContent banners = Model.Content.Children.Where(e => e.Name.Contains("Banners")).FirstOrDefault();
    IPublishedContent contentbanner01 = banners.Children.Where(e => e.Name.Contains("01")).FirstOrDefault();
    IPublishedContent contentbanner02 = banners.Children.Where(e => e.Name.Contains("02")).FirstOrDefault();
    IPublishedContent contentbanner03 = banners.Children.Where(e => e.Name.Contains("03")).FirstOrDefault();
    IPublishedContent contentbanner04 = banners.Children.Where(e => e.Name.Contains("04")).FirstOrDefault();
    IPublishedContent contentbanner05 = banners.Children.Where(e => e.Name.Contains("05")).FirstOrDefault();

    IPublishedContent imgbanner01 = Umbraco.Media(contentbanner01.GetPropertyValue<int>("img"));
    IPublishedContent imgbanner02 = Umbraco.Media(contentbanner02.GetPropertyValue<int>("img"));
    IPublishedContent imgbanner03 = Umbraco.Media(contentbanner03.GetPropertyValue<int>("img"));
    IPublishedContent imgbanner04 = Umbraco.Media(contentbanner04.GetPropertyValue<int>("img"));
    IPublishedContent imgbanner05 = Umbraco.Media(contentbanner05.GetPropertyValue<int>("img"));

    IPublishedContent development = Model.Content.Children.Where(e => e.Name.Contains("Development")).FirstOrDefault();

    IPublishedContent register = Model.Content.Children.Where(e => e.Name.Contains("Register")).FirstOrDefault();

    IPublishedContent LatestNews = Model.Content.Children.Where(e => e.Name.Contains("LatestNews")).FirstOrDefault();

    IPublishedContent user = Model.Content.Children.Where(e => e.Name.Contains("User")).FirstOrDefault();

    IPublishedContent partners = Model.Content.Children.Where(e => e.Name.Contains("Partners")).FirstOrDefault();

    IPublishedContent news = Model.Content.Ancestor(1).Children.Where(e => e.Name.Contains("News")).FirstOrDefault();
    IEnumerable<IPublishedContent> newsList = null;
    if (news != null && news.Children.Count() > 0)
    {
        newsList = news.Children().OrderByDescending(e => e.UpdateDate).Skip(0).Take(10);
    }
}

