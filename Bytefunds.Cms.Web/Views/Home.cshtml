                            <p><a href="@(Umbraco.Content(register.GetPropertyValue<int>("btnlink")).Url)" class="btn btn-lg btn-gray-transparent btn-animated" style="border-color: #FFFFFF">@(register.GetPropertyValue<string>("registerbtn"))<i class="fa fa-arrow-right pl-20"></i></a></p>
